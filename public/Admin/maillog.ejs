<div ng-app="grievances" ng-controller="Mail_Log">
    <h1>Mail Log</h1>
    <div id="mail" class="contenttable">
        <table class="table table-hover table-striped">
            <thead class="table-success">
                <tr>
                    <th>S.No.</th>
                    <th>Email ID</th>
                    <th>Subject</th>
                    <th>status</th>

                </tr> 
            </thead>
            <tr ng-repeat="mail in maillog">
                    <td>{{$index+1}}</td>
                    <td>{{mail.subject}}</td>
                    <td>{{mail.emailid}}</td>
                    <td>{{mail.status}}</td>
                </tr>
        </table>
    </div>
