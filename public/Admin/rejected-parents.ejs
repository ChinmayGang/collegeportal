<div ng-app="grievances" ng-controller="Admin_rejected_user" ng-init="init('parent')">
    <h1>Rejected Parents List</h1>
    <div id="rejected_list" class="contenttable">
        <table class="table table-hover table-striped">
            <thead class="table-success">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Email ID</th>
                    <th>Mobile No.</th>
                    <th>Relationship</th>
                    <th>Action</th>
                </tr> 
            </thead>
            <tr ng-repeat="user in users">
                <td>{{$index+1}}</td>
                <td>{{user.name}}</td>
                <td>{{user.emailid}}</td>
                <td>{{user.mobileno}}</td>
                <td>{{user.relation}}</td>
                <td>
                <button ng-click="Deactivate_user('Parent',user.emailid)" class="btn btn-danger">
                    Activate
                </button>
                </td>
            </tr>
        </table>
    </div>
    <div id="loading">
        <div class="loader">
        </div>
        
        <h1>Loading...</h1>
    </div>

</div>

