

		<!--Parent-->

		<h1>Edit Parent Details</h1>
		<form ng-submit="submit()" class="form-horizontal">
			<div ng-app="grievances" ng-controller="Parent">
				<div class="form-group">
					<div class="row">
						<div class="col-sm-2 col-sm-offset-1">
							<label class="control-label"> Name </label>
						</div>
						<div class="col-sm-7">
							<input class="form-control" type="text" ng-model="name" readonly>
						</div>
					</div>
				</div>

				<div class="form-group">
					<div class="row">
						<div class="col-sm-2 col-sm-offset-1">
							<label class="control-label"> Relation </label>
						</div>
						<div class="col-sm-7">
							<input class="form-control" type="text" ng-model="relation" readonly>
						</div>
					</div>
				</div>

				<div class="form-group">
					<div class="row">
						<div class="col-sm-2 col-sm-offset-1">
							<label class="control-label"> Course Completion Date</label>
						</div>
						<div class="col-sm-7">
							<input class="form-control" type="text" ng-model="cdate" readonly>
						</div>
					</div>
				</div>
				<div class="form-group">
					<div class="row">
						<div class="col-sm-2 col-sm-offset-1">
							<label class="control-label"> Email Id</label>
						</div>
						<div class="col-sm-7">
							<input class="form-control" type="text" ng-model="email" readonly>
						</div>
					</div>
				</div>
				<div class="form-group">
					<div class="row">
						<div class="col-sm-2 col-sm-offset-1">
							<label class="control-label"> Mobile Number</label>
						</div>
						<div class="col-sm-7">
							<input class="form-control" type="text" ng-model="mobile" readonly>
						</div>
					</div>
				</div>
				<div class="form-group">
					<div class="col-sm-9 col-sm-offset-3">
						<button class="btn btn-default" type="submit" id="parent-details">Update</button>
					</div>
				</div>

			</div>

		</form>

