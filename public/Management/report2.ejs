<h1>Detailed Grievance Report</h1>
<div class="content" ng-app="grievances" ng-controller="Management_grievances_report">

<div class="form-group">
 <div class="row">
    <div class="col-sm-2 offset-sm-1">
     <label class="control-label"> From Date</label>
 </div>
 <div class="col-sm-7">
     <input class="form-control" type="date" name="from-date" value="2018-10-10">
 </div>
</div>
</div>
<div class="form-group">
    <div class="row">
        <div class="col-sm-2 offset-sm-1">
         <label class="control-label"> To Date</label>
     </div>
     <div class="col-sm-7">
         <input class="form-control" type="date" name="to-date" value="2018-10-10">
     </div>
 </div>
</div>
<div class="form-group">
    <div class="col-sm-7 offset-sm-3">
        <button class="btn btn-default" type="submit" id="student-details" ng-click="Detail()" >Create</button>
 </div>
</div>
</div>